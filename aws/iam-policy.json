{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "ListBuildArtifactsBucket",
      "Effect": "Allow",
      "Action": [
        "s3:ListBucket",
        "s3:GetBucketLocation"
      ],
      "Resource": "arn:aws:s3:::build-artifacts-bucket"
    },
    {
      "Sid": "ReadBuildArtifacts",
      "Effect": "Allow",
      "Action": [
        "s3:GetObject",
        "s3:GetObjectVersion"
      ],
      "Resource": "arn:aws:s3:::build-artifacts-bucket/*"
    },
    {
      "Sid": "ManageDeployWebUIBucket",
      "Effect": "Allow",
      "Action": [
        "s3:ListBucket",
        "s3:GetBucketLocation"
      ],
      "Resource": "arn:aws:s3:::deploy-webui-bucket"
    },
    {
      "Sid": "DeployWebUIObjects",
      "Effect": "Allow",
      "Action": [
        "s3:PutObject",
        "s3:PutObjectAcl",
        "s3:DeleteObject",
        "s3:GetObject"
      ],
      "Resource": "arn:aws:s3:::deploy-webui-bucket/*"
    }
  ]
}
